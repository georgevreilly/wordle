#!/bin/bash

# shellcheck disable=SC2016
IFS=' ' read -r -a guess_scores < <(grep "$1" README.md | sed 's/[^`]*`\([^`]*\)`.*/\1/')

IFS=$'\n' read -d '' -r -a guess_scores < <(grep "$1" README.md | grep -o ".....=....." && printf '\0')
echo "${#guess_scores[@]}: ${guess_scores[@]}"
./wordle.py "${guess_scores[@]}"
